{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "object-curly-newline": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off"
  }
}
